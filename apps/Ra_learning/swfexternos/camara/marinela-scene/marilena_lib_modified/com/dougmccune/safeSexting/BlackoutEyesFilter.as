package com.dougmccune.safeSexting{	import flash.display.Sprite;	import flash.geom.Rectangle;	public class BlackoutEyesFilter implements IFaceFilter	{		public function BlackoutEyesFilter()		{		}		public function applyFilter(fullWebcam:Sprite, faceOverlayOnly:Sprite):void		{			fullWebcam.filters = [];		}				public function runFilter(fullWebcam:Sprite, faceOverlayOnly:Sprite, percentageRectangle:Rectangle):void		{			faceOverlayOnly.graphics.clear();			faceOverlayOnly.graphics.beginFill(0,1);						var x:Number = faceOverlayOnly.width * percentageRectangle.x;			trace("dhdhdhdhdhdhdhdhdhdhdhdhdhdhdhdh");			var y:Number = faceOverlayOnly.height * percentageRectangle.y;						var w:Number = faceOverlayOnly.width * percentageRectangle.width;			var h:Number = faceOverlayOnly.height * percentageRectangle.height;						y = y + h/3;			h = h/3.5;					faceOverlayOnly.graphics.drawRect(x, y, w, h);						faceOverlayOnly.graphics.endFill();		}			}}